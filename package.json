{
  "name": "rollup-tailwind-test",
  "version": "1.1.26",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./dist/styles.css": "./dist/styles.css"
  },
  "sideEffects": [
    "dist/styles.css"
  ],
  "files": [
    "dist",
    "dist/assets"
  ],
  "scripts": {
    "clean": "rimraf dist src/styles/dist || true",
    "build": "rollup -c",
    "prepack": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25",
    "@rollup/plugin-node-resolve": "^15",
    "@rollup/plugin-url": "^8",
    "@tailwindcss/postcss": "^4",
    "autoprefixer": "^10",
    "esbuild": "^0.21",
    "postcss-url": "^10",
    "rollup": "^4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^2",
    "rollup-plugin-esbuild": "^6",
    "rollup-plugin-postcss": "^4"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"

  }
}
