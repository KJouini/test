{
  "name": "rollup-tailwind-test",
  "version": "1.1.25",
  "description": "Librairie de composants React avec Tailwind CSS v4, Rollup",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "style": "dist/styles.css",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf dist src/styles/dist",
    "prepack": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25",
    "@rollup/plugin-node-resolve": "^15",
    "@rollup/plugin-url": "^8",
    "@tailwindcss/postcss": "^4",
    "autoprefixer": "^10",
    "esbuild": "^0.21",
    "postcss-url": "^10",
    "rimraf": "^6.0.1",
    "rollup": "^4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^2",
    "rollup-plugin-esbuild": "^6",
    "rollup-plugin-postcss": "^4"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
